<div class="panel-body">
  <table class="table table-striped table-bordered table-list">
    <thead>
      <tr>
        <th><em class="glyphicon glyphicon-cog"></em></th>
        <th><%= t ".name" %></th>
        <th><%= t ".email" %></th>
      </tr>
    </thead>
    <tbody class="users">
      <%= render @users %>
    </tbody>
  </table>
</div>
<div class="panel-footer">
  <%= will_paginate @users %>
</div>
